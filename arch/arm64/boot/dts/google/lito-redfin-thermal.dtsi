/*
 * Rename bmr_w_therm to mmw_side_therm
 */
&pm7250b_vadc {
	/delete-node/ bmr_w_therm@53;
	disp_therm@4e {
		reg = <ADC_AMUX_THM2_PU2>;
		label = "disp_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	mmw_side_therm@53 {
		reg = <ADC_GPIO2_PU2>;
		label = "mmw_side_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&pm7250b_adc_tm {
	io-channels = <&pm7250b_vadc ADC_AMUX_THM1_PU2>,
			<&pm7250b_vadc ADC_AMUX_THM2_PU2>,
			<&pm7250b_vadc ADC_AMUX_THM3_PU2>,
			<&pm7250b_vadc ADC_GPIO2_PU2>;

	/delete-node/ bmr_w_therm@53;
	disp_therm@4e {
		reg = <ADC_AMUX_THM2_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};

	mmw_side_therm@53 {
		reg = <ADC_GPIO2_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};
};

/*
 * Rename bmr_e_therm to disp_conn_therm
 * Rename pa_therm1 to pa_therm_1
 */
&pm8150_vadc {
	/delete-node/ bmr_e_therm@4e;
	/delete-node/ pa_therm1@4d;

	disp_conn_therm@4e {
		reg = <ADC_AMUX_THM2_PU2>;
		label = "disp_conn_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	pa_therm_1@4d {
		reg = <ADC_AMUX_THM1_PU2>;
		label = "pa_therm_1";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

/*
 * Rename bmr_e_therm to disp_conn_therm
 * Rename pa_therm1 to pa_therm_1
 */
&pm8150_adc_tm {
	io-channels = <&pm8150_vadc ADC_XO_THERM_PU2>,
			<&pm8150_vadc ADC_AMUX_THM1_PU2>,
			<&pm8150_vadc ADC_AMUX_THM2_PU2>;

	/delete-node/ bmr_e_therm@4e;
	/delete-node/ pa_therm1@4d;

	disp_conn_therm@4e {
		reg = <ADC_AMUX_THM2_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};

	pa_therm_1@4d {
		reg = <ADC_AMUX_THM1_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};
};

/*
 * Rename pa_therm2 to skin_therm
 * Rename msm_skin_therm to sdm_therm
