			};
		};
	};

	pa-therm1-usr {
		status = "disabled";
	};

	pa-therm2-usr {
		status = "disabled";
	};

	mmw-pa1-usr {
		status = "disabled";
	};

	mmw-pa2-usr {
		status = "disabled";
	};

	mmw-pa3-usr {
		status = "disabled";
